{
    "name": "oitmain/yii2-smartcron-v1",
    "description": "Smart cron module for Yii2",
    "keywords": ["yii2", "yii2-user", "cron", "oitmain"],
    "type": "yii2-extension",
    "license": "Internal",
    "support": {
        "issues": "https://github.com/oitmain/oitmain-yii2-smartcron-v1/issues?state=open",
        "source": "https://github.com/oitmain/oitmain-yii2-smartcron-v1"
    },
    "authors": [
        {
            "name": "Oitmain, Inc",
            "email": "server@oitmain.com",
            "homepage": "http://oitmain.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-bootstrap": "~2.0",
        "mtdowling/cron-expression": "1.0.4"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "oitmain\\yii2\\smartcron\\v1\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}